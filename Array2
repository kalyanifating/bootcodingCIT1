public class Array2{ 
    public static void main(String[] args) {
        String a="sham";
        String name[]={"sham","ram","sita","gita"};
        int number[]={23,49,46,38};
        System.out.println(number[3]);
        for(int i=0; i<=number,length; i++){
            System.out.println(number[i]);
        }
        
    }
}
